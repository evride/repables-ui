{
   "parser":"@babel/eslint-parser",
   "parserOptions":{
      "sourceType":"module",
      "allowImportExportEverywhere":false,
      "codeFrame":false
   },
   "extends":[
      "airbnb",
      "prettier"
   ],
   "env":{
      "browser":true,
      "jest":true
   },
   "rules":{
      "max-len":[
         "error",
         {
            "code":100
         }
      ],
      "prefer-promise-reject-errors":[
         "off"
      ],
      "react/jsx-filename-extension":[
         "off"
      ],
      "react/prop-types":[
         "warn"
      ],
      "no-return-assign":[
         "off"
      ],
      "jsx-a11y/label-has-associated-control":[
          "off"
      ],
      "no-plusplus":[
          "off"
      ],
      "no-param-reassign":[
        "off"
      ],
      "no-console":[
        "off"
      ],
      "arrow-body-style": [
         "off"
      ],
      "prefer-template": [
         "off"
      ]

   }
}
